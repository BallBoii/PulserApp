{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
"permissions": [
    "core:default",
    "core:window:default", 
    "core:path:default",
    "shell:default",
    "shell:allow-open",
    "shell:allow-execute",
    "shell:allow-spawn"
  ]
}
